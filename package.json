{
  "devDependencies": {
    "elm-format": "^0.8.1",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5"
  },
  "scripts": {
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.elm": [
      "elm-format --yes",
      "git add"
    ],
    "*.js": [
      "prettier.js --write",
      "git add"
    ]
  }
}
