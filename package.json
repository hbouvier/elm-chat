{
  "devDependencies": {
    "elm-format": "^0.8.1",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5"
  },
  "scripts": {},
  "lint-staged": {
    "src/**/*.elm": [
      "elm-format --yes",
      "git add"
    ],
    "*.js": [
      "prettier.js --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
